<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/dark_brown"
    tools:context=".splashscreen.SplashScreenActivity">

    <ImageView
        android:id="@+id/iv_logo_1"
        android:layout_width="220dp"
        android:layout_height="198dp"
        android:src="@drawable/ic_splash_screen1"
        android:contentDescription="@string/iv_logo_1"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:padding="10dp"
        android:layout_marginTop="108dp"/>
    <ImageView
        android:id="@+id/iv_logo_2"
        android:layout_width="188dp"
        android:layout_height="200dp"
        android:src="@drawable/ic_splash_screen2"
        android:contentDescription="@string/iv_logo_2"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/iv_logo_1"
        android:padding="10dp"
        android:layout_marginTop="10dp"/>

</androidx.constraintlayout.widget.ConstraintLayout>